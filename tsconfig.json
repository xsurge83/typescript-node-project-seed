{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "noEmitHelpers": false,
    "target": "ES5",
    "outDir": "./"
  },
  "files": [
    "typings/main.d.ts"
  ],
  "exclude": [
    "node_modules",
    "tests"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
